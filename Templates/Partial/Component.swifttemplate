final class <%= component.implementationName %>: <%= component.interfaceName %> {
  <% for member in component.members { %>
    private let <%= member.name %>: <%= member.typeName %>
  <% } %>

    init() {
      <% for member in component.members { %>
        <% factory = Factory(member.type) %>
        <%= member.name %> = <%= factory.implementationName %>(
          <%= factory.members.map { "\($0.name): \($.name)" }.joined(separator: ", ") %>
        )
      <% } %>
    }
}
