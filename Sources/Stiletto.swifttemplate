class Provider<T> {
    var instance: T { fatalError() } // To Be Overriden
}

<%_ do { let stiletto = try Stiletto(types) -%>
  <%_ for component in stiletto.components { -%>
    <%_- include("Partial/Component.swifttemplate") %>
  <%_ } -%>
  <%_ for factory in stiletto.factories { -%>
    <%_- include("Partial/Factory.swifttemplate") %>
  <%_ } -%>
<%_ } catch let error { -%>
// Stiletto Generation Failed
// <%= error %>
<%_ } -%>
